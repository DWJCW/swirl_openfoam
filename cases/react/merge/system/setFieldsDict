FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 300
);


regions
(
    boxToCell
    {
        box    (-1 -1 -1) (1 1 1);

        fieldValues
        (
            volScalarFieldValue T 1273
        );
    }
);



